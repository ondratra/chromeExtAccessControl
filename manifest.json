{
    "manifest_version": 2,

    "name": "Chrome Extension - Access Control",
    "description": "In development",
    "version": "1.0",

    "background": {
        "scripts": ["./build.js"]
    },

    "permissions": [
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ]
}
