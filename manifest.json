{
    "manifest_version": 2,

    "name": "Chrome Extension - Access Control",
    "description": "In development",
    "version": "1.0",

    "background": {
        "scripts": ["./build.js"]
    },

    "browser_action": {
        "default_title": "Chrome Extension - Access Control",
        "default_popup": "gui/gui.html"
    },

    "permissions": [
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>",
        "storage"
    ]
}
